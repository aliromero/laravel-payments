{
    "name": "aliromero/laravel-payments",
    "description": "package multi payments for Laravel Framework.",
    "keywords": ["payment", "laravel", "zarinpal", "idpay", "payir", "zibal"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ali romero",
            "email": "ali.rom3ro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.1"
    },
    "autoload": {
        "psr-4": {
            "Romero\\LaravelPayments\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Romero\\LaravelPayments\\Providers\\PaymentServiceProvider"
            ],
            "aliases": {
                "Payments": "Romero\\LaravelPayments\\Facades\\Payments"
            }
        }
    }
}
